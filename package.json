{
  "name": "yoga-studio-web",
  "version": "1.0.0",
  "description": "Веб-приложение для управления йога-студией с админ-панелью",
  "private": true,
  "scripts": {
    "install:all": "npm install && cd client && npm install",
    "client:dev": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "server:dev": "cd server && npm run dev",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run client:build",
    "deploy": "npm run build && gh-pages -d client/build"
  },
  "keywords": [
    "yoga",
    "studio",
    "admin",
    "react",
    "typescript",
    "tailwind"
  ],
  "author": "Ambaklya",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "gh-pages": "^6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ambaklya/yoga-studio-web.git"
  },
  "homepage": "https://ambaklya.github.io/yoga-studio-web"
}
